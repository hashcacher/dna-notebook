# Step 1: Research and select appropriate front-end and back-end technologies
# For this example, we'll use Flask for the back-end and React for the front-end

from flask import Flask, jsonify

app = Flask(__name__)

# Step 3: Implement the back-end functionality for storing and retrieving DNA sequence data
sequences = {
    "seq1": "ATCGCGATCGATCGATCG",
    "seq2": "CGTAGCTAGCTACGTAGCT"
}

@app.route('/api/sequences', methods=['GET'])
def get_sequences():
    return jsonify(sequences)

if __name__ == '__main__':
    app.run(debug=True)
